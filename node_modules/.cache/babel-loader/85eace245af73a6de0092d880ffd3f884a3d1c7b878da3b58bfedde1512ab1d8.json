{"ast":null,"code":"import NotificationTemplate from \"./Notifications/NotificationTemplate\";\nimport { BaseAlert } from \"@/components\";\nexport default {\n  components: {\n    BaseAlert\n  },\n  data() {\n    return {\n      type: [\"\", \"info\", \"success\", \"warning\", \"danger\"],\n      notifications: {\n        topCenter: false\n      }\n    };\n  },\n  methods: {\n    notifyVue(verticalAlign, horizontalAlign) {\n      const color = Math.floor(Math.random() * 4 + 1);\n      this.$notify({\n        component: NotificationTemplate,\n        icon: \"tim-icons icon-bell-55\",\n        horizontalAlign: horizontalAlign,\n        verticalAlign: verticalAlign,\n        type: this.type[color],\n        timeout: 0\n      });\n    }\n  }\n};","map":{"version":3,"names":["NotificationTemplate","BaseAlert","components","data","type","notifications","topCenter","methods","notifyVue","verticalAlign","horizontalAlign","color","Math","floor","random","$notify","component","icon","timeout"],"sources":["src/pages/Notifications.vue"],"sourcesContent":["<template>\n  <div class=\"row\">\n    <div class=\"col-md-6\">\n      <card>\n        <h4 slot=\"header\">Notifications Style</h4>\n        <base-alert type=\"info\">\n          <span>This is a plain notification</span>\n        </base-alert>\n        <base-alert type=\"info\" dismissible>\n          <span>This is a plain notification</span>\n        </base-alert>\n        <base-alert type=\"info\" dismissible with-icon>\n          <span data-notify=\"icon\" class=\"tim-icons icon-bell-55\"></span>\n          <span data-notify=\"message\"\n            >This is a notification with close button and icon.</span\n          >\n        </base-alert>\n        <base-alert type=\"info\" dismissible with-icon>\n          <span data-notify=\"icon\" class=\"tim-icons icon-bell-55\"></span>\n          <span data-notify=\"message\"\n            >This is a notification with close button and icon and have many\n            lines. You can see that the icon and the close button are always\n            vertically aligned. This is a beautiful notification. So you don't\n            have to worry about the style.</span\n          >\n        </base-alert>\n      </card>\n    </div>\n    <div class=\"col-md-6\">\n      <card>\n        <h4 slot=\"header\">Notifications states</h4>\n        <base-alert type=\"primary\" dismissible>\n          <span\n            ><b> Primary - </b> This is a regular notification made with\n            \".alert-primary\"</span\n          >\n        </base-alert>\n        <base-alert type=\"info\" dismissible>\n          <span\n            ><b> Info - </b> This is a regular notification made with\n            \".alert-info\"</span\n          >\n        </base-alert>\n        <base-alert type=\"success\" dismissible>\n          <span\n            ><b> Success - </b> This is a regular notification made with\n            \".alert-success\"</span\n          >\n        </base-alert>\n        <base-alert type=\"warning\" dismissible>\n          <span\n            ><b> Warning - </b> This is a regular notification made with\n            \".alert-warning\"</span\n          >\n        </base-alert>\n        <base-alert type=\"danger\" dismissible>\n          <span\n            ><b> Danger - </b> This is a regular notification made with\n            \".alert-danger\"</span\n          >\n        </base-alert>\n      </card>\n    </div>\n    <div class=\"col-md-12\">\n      <card>\n        <div class=\"places-buttons\">\n          <div class=\"row\">\n            <div class=\"col-md-6 ml-auto mr-auto text-center\">\n              <h4 class=\"card-title\">\n                Notifications Places\n                <p class=\"category\">Click to view notifications</p>\n              </h4>\n            </div>\n          </div>\n          <div class=\"row\">\n            <div class=\"col-lg-8 ml-auto mr-auto\">\n              <div class=\"row\">\n                <div class=\"col-md-4\">\n                  <base-button\n                    type=\"primary\"\n                    block\n                    @click=\"notifyVue('top', 'left')\"\n                    >Top Left</base-button\n                  >\n                </div>\n                <div class=\"col-md-4\">\n                  <base-button\n                    type=\"primary\"\n                    block\n                    @click=\"notifyVue('top', 'center')\"\n                    >Top Center</base-button\n                  >\n                </div>\n                <div class=\"col-md-4\">\n                  <base-button\n                    type=\"primary\"\n                    block\n                    @click=\"notifyVue('top', 'right')\"\n                    >Top Right</base-button\n                  >\n                </div>\n              </div>\n            </div>\n          </div>\n          <div class=\"row\">\n            <div class=\"col-lg-8 ml-auto mr-auto\">\n              <div class=\"row\">\n                <div class=\"col-md-4\">\n                  <base-button\n                    type=\"primary\"\n                    block\n                    @click=\"notifyVue('bottom', 'left')\"\n                    >Bottom Left</base-button\n                  >\n                </div>\n                <div class=\"col-md-4\">\n                  <base-button\n                    type=\"primary\"\n                    block\n                    @click=\"notifyVue('bottom', 'center')\"\n                    >Bottom Center</base-button\n                  >\n                </div>\n                <div class=\"col-md-4\">\n                  <base-button\n                    type=\"primary\"\n                    block\n                    @click=\"notifyVue('bottom', 'right')\"\n                    >Bottom Right</base-button\n                  >\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </card>\n    </div>\n  </div>\n</template>\n<script>\nimport NotificationTemplate from \"./Notifications/NotificationTemplate\";\nimport { BaseAlert } from \"@/components\";\n\nexport default {\n  components: {\n    BaseAlert,\n  },\n  data() {\n    return {\n      type: [\"\", \"info\", \"success\", \"warning\", \"danger\"],\n      notifications: {\n        topCenter: false,\n      },\n    };\n  },\n  methods: {\n    notifyVue(verticalAlign, horizontalAlign) {\n      const color = Math.floor(Math.random() * 4 + 1);\n      this.$notify({\n        component: NotificationTemplate,\n        icon: \"tim-icons icon-bell-55\",\n        horizontalAlign: horizontalAlign,\n        verticalAlign: verticalAlign,\n        type: this.type[color],\n        timeout: 0,\n      });\n    },\n  },\n};\n</script>\n<style></style>\n"],"mappings":"AA4IA,OAAAA,oBAAA;AACA,SAAAC,SAAA;AAEA;EACAC,UAAA;IACAD;EACA;EACAE,KAAA;IACA;MACAC,IAAA;MACAC,aAAA;QACAC,SAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,UAAAC,aAAA,EAAAC,eAAA;MACA,MAAAC,KAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAE,MAAA;MACA,KAAAC,OAAA;QACAC,SAAA,EAAAhB,oBAAA;QACAiB,IAAA;QACAP,eAAA,EAAAA,eAAA;QACAD,aAAA,EAAAA,aAAA;QACAL,IAAA,OAAAA,IAAA,CAAAO,KAAA;QACAO,OAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}